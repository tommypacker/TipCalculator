<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tommypacker.materialtipper" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.tommypacker.materialtipper.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.tommypacker.materialtipper.DisplayBill"
            android:label="Final Bill" />
        <activity android:name="com.tommypacker.materialtipper.Settings.settings"
            android:label="About App"
            android:parentActivityName="com.tommypacker.materialtipper.MainActivity" >
        </activity>
    </application>

</manifest>
